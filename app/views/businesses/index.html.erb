<p id="notice"><%= notice %></p>

<h1>Businesses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Street</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Phone</th>
      <th>Zipcode</th>
      <th>Email</th>
      <th>Facebook</th>
      <th>Instagram</th>
      <th>Youtube</th>
      <th>Twitter</th>
      <th>Logo</th>
      <th>Operator</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @businesses.each do |business| %>
      <tr>
        <td><%= business.name %></td>
        <td><%= business.street %></td>
        <td><%= business.city %></td>
        <td><%= business.state %></td>
        <td><%= business.country %></td>
        <td><%= business.phone %></td>
        <td><%= business.zipcode %></td>
        <td><%= business.email %></td>
        <td><%= business.facebook %></td>
        <td><%= business.instagram %></td>
        <td><%= business.youtube %></td>
        <td><%= business.twitter %></td>
        <td><%= business.logo %></td>
        <td><%= business.operator %></td>
        <td><%= link_to 'Show', business %></td>
        <td><%= link_to 'Edit', edit_business_path(business) %></td>
        <td><%= link_to 'Destroy', business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business', new_business_path %>
